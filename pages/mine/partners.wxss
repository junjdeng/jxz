/* pages/mine/partners.wxss */
page{background: #f5f5f5;}
.fixed_header{position: fixed; top: 0; z-index: 99; width: 100%;}
.partners_header{background: linear-gradient(2deg, #3478FF,#00B9FF); height: 360rpx;width: 100%; text-align: center; color: #fff; font-size: 28rpx; display: flex; flex-direction: column; justify-content: center;}
.partners_header .income{font-size: 48rpx;}
.partners_header .withdraw{font-size: 32rpx; text-align: center; margin-top: 20rpx; padding: 20rpx;}
.partners_header .withdraw .btn{border: 1px solid #d5d5d5; padding: 10rpx; border-radius: 8rpx; height: 90rpx;}
.jx_tabs{width: 100%; background: #fff; }
.list{margin-top: 460rpx; width: 100%;}

.list_partner_item{background: #fff; height: 100rpx; margin: 0 20rpx 20rpx 20rpx; padding: 20rpx; border-radius: 8rpx;}
.list_partner_item .name{font-size: 32rpx; line-height: 60rpx; color: #302C3D;}
.list_partner_item .time{font-size: 20rpx; line-height: 40rpx; color: #979797;}
.list_partner_item .status{font-size: 28rpx; color: #FF4F00;}

.cover{position: fixed; top: 0; left: 0; z-index: 100; height: 100%; width: 100%; background-color: rgba(0, 0, 0, 0.3);}
.modal{background: #fff; width: 400rpx; margin: 300rpx auto; padding: 20rpx; border-radius: 8rpx; color: #999999;
font-size:24rpx; line-height: 50rpx;text-align: center;}
.modal .title{color: #333; font-size:32rpx; font-weight: 600;}
.modal image{width: 160rpx; height: 160rpx; position: relative; top: -80rpx;}
.modal button{margin: 50rpx 100rpx; height: 70rpx; border-radius: 35rpx; background: #3478FF; font-size: 28rpx; color: #fff; }