page{background: #f5f5f5;}

.list_header{height: 320rpx; width: 100%; color: #fff;background-size:100% 100%;background-repeat:no-repeat;}
.list_header .title{font-size: 36rpx; line-height: 50rpx;}

.section_card{background: #fff; margin:20rpx; border-radius: 8rpx; width: 690rpx; font-size: 32rpx; line-height: 44rpx;color: #302C3D; padding:20rpx 10rpx;}
.section_card image{width: 110rpx; height: 110rpx; border-radius: 50%; margin-right: 20rpx; }
.section_card span{color: #FF4F00;}
.section_card .subtitle{font-size: 28rpx; line-height: 50rpx;}
.section_card .time{font-size: 28rpx; line-height: 50rpx; position:relative;top:-30rpx;}

.active_card{background: #fff; margin:0 20rpx; border-radius: 8rpx; width: 710rpx;}
.active_card image{height: 240rpx; width: 240rpx; margin-right: 20rpx; border-radius: 8rpx;}
.active_card .card_content{display: flex; flex-direction: column; justify-content: space-between; height: 240rpx;}
.active_card .card_content .title{font-size: 32rpx; line-height: 44rpx; color: #302C3D; font-weight: 600;}
.active_card .card_content .price{font-size: 36rpx; line-height: 50rpx; color: #FF4F00;}
.active_card .card_content .subtitle{font-size: 28rpx; line-height: 50rpx; color: #00AE11;}


.detail_data{margin:0 20rpx 0 20rpx; width: 670rpx; background: #fff; padding: 20rpx; text-align: center;font-size: 24rpx; line-height: 45rpx;}
.detail_data .num{color: #FF4F00; font-size: 32rpx; line-height: 65rpx;}

.fund_progress_wrap{width: 690rpx; padding: 0 10rpx; height: 50rpx; background: #fff;}
.fund_progress_title{color: #302C3D; font-size: 28rpx; padding-right:120rpx;}
.fund_progress_title span{float: right;}
.fund_progress{color: red;}

.group_card{margin: 20rpx; width: 670rpx; background: #fff; padding: 20rpx;}
.group_card .group_name{font-size: 32rpx; line-height: 44rpx; color: #302C3D;}
.group_card .group_icons image{width: 110rpx; height: 110rpx; border-radius: 50%; margin-right: 25rpx; margin-top: 10rpx;}

.range_list{width: 710rpx; margin: 0 20rpx;}
.range_list .jx_tabs{width:100%; background: #fff;}
.range_list .list_item{background: #fff; height: 140rpx; padding: 0 40rpx; margin-top: 2rpx;color: #302C3D; font-size: 28rpx; line-height: 40rpx;}
.range_list .list_item i-icon{position: relative; top: 25rpx;}
.range_list .list_item image{width: 110rpx; height: 110rpx; border-radius: 50%; float: right; margin-right: 40rpx;}
.range_list .list_item .comment{font-size: 20rpx;}
.range_list .list_item .counts{color: #FF4F00; text-align: right;}

.checkMore{width: 710rpx; margin: 0 20rpx;font-size: 32rpx; line-height: 44rpx; background: #fff; color: #3478FF;text-align: center; margin-top: 2rpx; padding: 10rpx 0;}

.support_list{margin: 0 20rpx; width: 710rpx; background: #fff; margin-bottom: 130rpx;}
.support_list .list_item{background: #fff; height: 140rpx; padding: 0 40rpx; margin-top: 2rpx;
color: #302C3D; font-size: 28rpx; line-height: 40rpx;}
 .support_list .list_item image{width: 110rpx; height: 110rpx; border-radius: 50%;}
 .support_list .list_item .comment{font-size: 20rpx;}
 .support_list .list_item .counts{align-self: flex-end; padding-bottom: 20rpx; text-align: right;} 

 .home{position: fixed; bottom: 150rpx; width: 60rpx; height: 60rpx; border-radius: 50rpx; background: #ccc; padding: 20rpx; left: 40rpx; }
 .home image{width: 100%; height: 100%;}

 .actions{position: fixed; bottom: 0; width: 710rpx; background: #fff; padding: 10rpx 20rpx; }
.actions button{background: #e1ffe4; color:#00AE11; margin-right: 10rpx; border: 1px solid #00AE11;}
.actions .button1{background: #EEF6FF; color:#3478FF;  border: 1px solid #3478FF;}
.actions .button2{background: #FFF8EB; color:#FF4F00;  border: 1px solid #FF4F00;}